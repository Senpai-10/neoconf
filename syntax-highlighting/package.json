{
    "name": "neoconf",
    "displayName": "neoconf",
    "description": "neoconf syntax highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "neoconf",
            "aliases": ["neoconf", "neoconf"],
            "extensions": [".neoconf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "neoconf",
            "scopeName": "config.neoconf",
            "path": "./syntaxes/neoconf.tmLanguage.json"
        }]
    }
}